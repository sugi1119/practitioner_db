<h2>Practitioner index page</h2>
<!-- List the practitioners -->
<div class="col-md-7 col-md-offset-2">
<% @practitioners.each do |practitioner| %>
  <ul class="p_list">
    <blockquote>
      <li>Name: <%= practitioner.name %></li>
      <li>Contact number: <%= practitioner.phone %></li>
      <li>e-mail: <%= practitioner.email %></li>
      <li></li>

      <li><%= link_to('Edit', edit_practitioner_path(practitioner))%></li>
      <li><%= link_to('Delete', practitioner, :data => {:confirm => 'Please confirm to delete the practitioner from the list', :method => :delete })%></li>
    </blockquote>
  </ul>
  <hr>
<% end %>
</div>